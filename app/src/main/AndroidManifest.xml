<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.omkabel.diagnosa_penyakit">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_icon"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@drawable/ic_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.DiagnosaPenyakit">
        <activity android:name=".Features.Menu_Setting"></activity>
        <activity android:name=".Features.Menu_History" />
        <activity android:name=".Features.Menu_Monitoring" />
        <activity android:name=".Features.Menu_UpdateProfile" />
        <activity android:name=".Features.Menu_ListPenyakit" />
        <activity android:name=".Features.Menu_DiagNosa" />
        <activity android:name=".Features.Menu_About" />
        <activity android:name=".Features.Menu_Profile" />
        <activity android:name=".Features.Menu_Dashboard" />
        <activity android:name=".Features.Menu_Register" />
        <activity android:name=".Features.Menu_Login" />
        <activity android:name=".Features.Menu_SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>